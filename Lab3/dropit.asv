function dropit(id)
%Centre angle of the base motor
calllib('dynamixel', 'dxl_write_word', id, 30, 0);
while(calllib('dynamixel', 'dxl_read_word', 4, 46)>0);
pause(0.1)
end
calllib('dynamixel', 'dxl_write_word', id, 30, 2500);
while(calllib('dynamixel', 'dxl_read_word', 4, 46)>0);
end
calllib('dynamixel', 'dxl_write_word', id, 30, 0);
pause(0.1)
while(calllib('dynamixel', 'dxl_read_word', 4, 46)>0);
end
calllib('dynamixel', 'dxl_write_word', id, 30, 2500);

end